<div class="card">
  <div class="m-4 income_statement">
    <%= form_tag admin_analysts_path, method: :get, class: "form-inline float-right" do |f| %>
      <div class="col">
        <%= text_field_tag :date_range, @date_range, class: "form-control", autocomplete: 'off' %>
        <%= submit_tag 'Search', class: "btn btn-primary" %>
      </div>
    <% end %>
  </div>
</div>




<div class="card">
  <%= table_for @trades, class: 'table table-condensed table-striped' do |t| %>
    <% t.column 'Member' do |x| %>
      <%= x.ask_member_id  %>
    <% end %>

    <% t.column 'Symbol' do |x| %>
      <%= x.market_id %>
    <% end %>

    <% t.column 'Amount' do |x| %>
      <span class="h5"><%= x.funds %></span>
    <% end %> 
  <% end %>

  <%= paginate @trades %>
</div>
 
